<routes xmlns="http://camel.apache.org/schema/spring">

	<route id="start-hazelcast-queue-timer" errorHandlerRef="defaultEH">
		<from uri="timer:foo?period=5000" />
		<log message="Producer side: Sending data to Hazelcast Queue" />
		<setBody><constant>HELLO</constant></setBody>
		<to uri="hazelcast-queue:foo" />
		<log message="Producer side: Sended data Async" />
	</route>


	<route id="start-hazelcast-queue-route" errorHandlerRef="defaultEH">
		<from uri="hazelcast-queue:foo" />
		<log message="Getting Message From queue foo ${body}" />
		
	</route>

</routes>