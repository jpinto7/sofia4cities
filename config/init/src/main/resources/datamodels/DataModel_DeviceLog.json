{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "DeviceLog",
    "type": "object",
    "required": [
        "DeviceLog"
    ],
    "properties": {
        "DeviceLog": {
            "type": "string",
            "$ref": "#/datos"
        }
    },
    "datos": {
        "description": "Info DeviceLog",
        "type": "object",
        "required": [
            "Status",
            "Level",
            "Message",
            "Device",
            "Timestamp"
        ],
        "properties": {
            "Coordinates": {
                "type": "object",
                "required": [
                    "coordinates",
                    "type"
                ],
                "properties": {
                    "coordinates": {
                        "type": "object",
                        "properties": {
                            "latitude": {
                                "type": "number"
                            },
                            "longitude": {
                                "type": "number"
                            }
                        }
                    },
                    "type": {
                        "type": "string",
                        "enum": [
                            "Point"
                        ]
                    }
                }
            },
            "Status": {
                "type": "string"
            },
            "Level": {
                "type": "string"
            },
            "Message": {
                "type": "string"
            },
            "ExtaOptions": {
                "type": "string"
            },
            "Device": {
                "type": "string"
            },
            "Timestamp": {
                "type": "object",
                "required": [
                    "$date"
                ],
                "properties": {
                    "$date": {
                        "type": "string",
                        "format": "date-time"
                    }
                }
            }
        }
    },
    "description": "Device logging",
    "additionalProperties": true
}